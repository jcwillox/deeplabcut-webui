{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:clean": "npm run clean && npm run build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom --watch false --passWithNoTests",
    "test:watch": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../.gitignore",
    "lint:fix": "npm run lint -s -- --fix",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check",
    "clean": "del-cli dist/"
  },
  "dependencies": {
    "@mdi/font": "^6.6.96",
    "@vueuse/core": "^8.2.6",
    "panzoom": "^9.4.2",
    "pinia": "^2.0.13",
    "roboto-fontface": "*",
    "video.js": "^7.18.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vuetify": "^3.0.0-beta.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.2",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@types/video.js": "^7.3.33",
    "@types/webfontloader": "^1.6.34",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "del-cli": "^4.0.1",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "0.34.7"
  },
  "packageManager": "pnpm@6.32.9",
  "engines": {
    "node": "16.x"
  }
}
