{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:clean": "npm run clean && npm run build",
    "preview": "vite preview --port 5050",
    "https-preview": "serve dist",
    "test": "vitest --environment jsdom --watch false --passWithNoTests",
    "test:watch": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../.gitignore --max-warnings=0",
    "lint:fix": "npm run lint -s -- --fix",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check",
    "clean": "del-cli dist/ node_modules/.vite/"
  },
  "dependencies": {
    "@mdi/font": "^6.7.96",
    "@panzoom/panzoom": "^4.5.0",
    "@vueuse/core": "^8.7.4",
    "deepmerge-ts": "^4.2.1",
    "hotkeys-js": "^3.9.4",
    "js-colormaps": "github:jcwillox/js-colormaps#78ee14d",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.6.1",
    "roboto-fontface": "*",
    "video.js": "^7.19.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vuetify": "^3.0.0-beta.4",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.41",
    "@types/video.js": "^7.3.40",
    "@types/webfontloader": "^1.6.34",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-vue": "^9.1.1",
    "https-localhost": "^4.7.1",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4",
    "vite": "^2.9.12",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vuetify": "1.0.0-alpha.12",
    "vitest": "^0.15.1",
    "vue-tsc": "^0.38.0",
    "workbox-window": "^6.5.3"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": "16.x"
  }
}
