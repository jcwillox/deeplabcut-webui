{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:clean": "npm run clean && npm run build",
    "preview": "vite preview --port 5050",
    "https-preview": "serve dist",
    "test": "vitest --environment jsdom --watch false --passWithNoTests",
    "test:watch": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../.gitignore --max-warnings=0",
    "lint:fix": "npm run lint -s -- --fix",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check",
    "clean": "del-cli dist/ node_modules/.vite/"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "@panzoom/panzoom": "^4.5.1",
    "@vueuse/core": "^9.3.0",
    "deepmerge-ts": "^4.2.2",
    "hotkeys-js": "^3.10.0",
    "js-colormaps": "github:jcwillox/js-colormaps#78ee14d",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "roboto-fontface": "*",
    "video.js": "^7.20.3",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.0-beta.13",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.65",
    "@types/video.js": "^7.3.48",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "https-localhost": "^4.7.1",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4",
    "vite": "^3.1.7",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-vuetify": "1.0.0-alpha.17",
    "vitest": "^0.24.1",
    "vue-tsc": "^1.0.3",
    "workbox-window": "^6.5.4"
  },
  "packageManager": "pnpm@7.13.4",
  "engines": {
    "node": "16.x"
  }
}
